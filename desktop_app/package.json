{
  "name": "electron-app",
  "version": "1.0.0",
  "description": "My Electron App for managing cookies",
  "main": "main.js",
  "scripts": {
    "start": "npx electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.example.myapp",
    "productName": "CookieManager",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
     
     
    ],
    "win": {
      "target": "nsis",
      "icon": "./icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "./icon.ico"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9"
   
  },
  "devDependencies": {
    "electron-builder": "^25.1.8",
    "electron": "^33.2.1"
   
  }
}
